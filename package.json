{
  "name": "@dmitry.olyenyov/effector-persist",
  "author": "Dmitry Olyenyov",
  "license": "MIT",
  "version": "0.0.3",
  "description": "allows to store effector's store into different storages (session, local)",
  "repository": "Dema/final-form-validations",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "unpkg": "dist/foo.umd.js",
  "source": "src/index.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle --compress",
    "dev": "microbundle watch --compress",
    "docs": "typedoc --out docs/api src",
    "prepublish": "run-p build docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --no-ignore --max-warnings=0 --fix",
      "git add"
    ],
    "*.{json}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^9.1.2",
    "@types/jest": "^26.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-eslint": "^10.0.3",
    "commitlint": "^9.1.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "microbundle": "^0.12.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "effector": "^21.5.0",
    "typedoc": "^0.18.0"
  },
  "peerDependencies": {
    "effector": "^21.5.0"
  }
}
